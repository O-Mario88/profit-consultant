{
  "name": "profit-driven-channel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:web": "vite",
    "dev:api": "node --env-file-if-exists=.env.local server/index.js",
    "dev:all": "concurrently -k -n api,web -c yellow,cyan \"npm:dev:api\" \"npm:dev:web\"",
    "start:api": "node --env-file-if-exists=.env.local server/index.js",
    "start": "SERVE_STATIC=true node --env-file-if-exists=.env.local server/index.js",
    "start:full": "npm run build && npm run start",
    "build": "vite build",
    "preview": "vite preview",
    "ai:sync-knowledge": "node scripts/sync_openai_knowledge.js",
    "ai:start-finetune": "node scripts/start_openai_finetune.js"
  },
  "dependencies": {
    "concurrently": "^9.2.1",
    "express": "^5.2.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.1.0",
    "lucide-react": "^0.563.0",
    "openai": "^6.21.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
